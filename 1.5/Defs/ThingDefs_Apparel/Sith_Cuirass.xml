			<useWornGraphicMask>true</useWornGraphicMask>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options" Inherit="False">
			<options>
				<li>
					<weight>10</weight>
					<only>(220, 220, 220)</only>
				</li>
			</options>
		</colorGenerator>
		<thingSetMakerTags>
			<li>OuterRim_ApparelSithEmpire</li>
		</thingSetMakerTags>
